{if(!canOpenPopup('ok'))return;var tpl='<div style="text-align:center;">';tpl+=message;tpl+='</div>';var popupWidth=300;var msg=jQuery('<div />').addClass('popupwindow popupwindow-ok').css({position:'absolute',left:parseInt(jQuery('body').width()/2-(popupWidth/2)),width:popupWidth,top:(window.pageYOffset?window.pageYOffset:document.documentElement.scrollTop)+200,display:'none'}).html('<div class="close_div close_div_ok"></div><p style="margin:0 auto;text-align: center">'+tpl+'</p>'+'<div class="popupbtns popupbtns-ok"></div>');jQuery('.close_div_ok',msg).append(jQuery('<img src="/images/close_popup.png" alt="close" id="close_button_showok" />').click(function()
{msg.fadeIn('slow',function()
{msg.remove();popupClosed('ok');});}).mousedown(function(e)
{e.cancelBubble=true;if(e.stopPropagation)
e.stopPropagation();}));jQuery('.popupbtns-ok',msg).append(jQuery('<input type="button" class="popupbutton ok" />').click(function(){msg.fadeIn('slow',function(){msg.remove();popupClosed('ok');});}));jQuery('body').append(msg);jQuery('.popupwindow-ok').draggable({handle:'.close_div_ok'});msg.fadeIn('slow');return false;}
