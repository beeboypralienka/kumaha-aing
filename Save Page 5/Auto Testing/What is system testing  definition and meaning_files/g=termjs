function get_lightbox_class(action_type,parameter_type)
{var class_type='membership-link membership-link-'+action_type;var parameter_type=typeof parameter_type!=='undefined'?parameter_type:'na';if(parameter_type=='default_register')
class_type='membership-link membership-register-link';else if(parameter_type=='default_login')
class_type='membership-link membership-login-link';else if(parameter_type=='login')
class_type=class_type+' membership-login-link';else if(parameter_type=='register')
class_type=class_type+' membership-register';else if($.cookie('user_has_logged_in_before'))
class_type=class_type+' membership-login-link';else
class_type=class_type+' membership-register';return class_type;}
