function htmlencode(str)
{str=''+str;var re=/\&/gi;str=str.replace(re,'&amp;');re=/"/gi;str=str.replace(re,'&quot;');re=/</gi;str=str.replace(re,'&lt;');re=/>/gi;str=str.replace(re,'&gt;');return str;}
function trim(str)
{str=''+str;var re=/^ */;var res=str.replace(re,'');re=/ *$/;return(res.replace(re,''));}
