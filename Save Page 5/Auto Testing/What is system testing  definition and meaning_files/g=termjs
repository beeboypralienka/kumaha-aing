{var splx=spl[i].split(':');switch(splx[0].toLowerCase())
{case'email':document.getElementById('feedback_email_error').innerHTML=splx[1];break;case'comments':document.getElementById('feedback_text_error').innerHTML=splx[1];break;}}
return false;}
