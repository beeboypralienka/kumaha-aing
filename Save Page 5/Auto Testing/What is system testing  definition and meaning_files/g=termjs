function AddEventHandler(obj,event,func){if(obj.attachEvent){obj.attachEvent("on"+event,func);}else if(obj.addEventListener)
obj.addEventListener(event,func,true);}
function RemoveEventHandler(obj,event,func){if(obj.attachEvent){obj.detachEvent("on"+event,func);}else if(obj.addEventListener)
obj.removeEventListener(event,func,true);}
