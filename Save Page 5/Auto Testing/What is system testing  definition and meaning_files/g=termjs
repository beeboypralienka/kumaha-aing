{var path='http://'+window.location.host+'/send_feedback.php';var res=Query('POST',path,'email='+escape(email)+'&comments='+escape(text),true);if(res.substr(0,2)=='ok')return true;if(res.substr(0,3)!='err')
{document.getElementById('feedback_email_error').innerHTML='Server error occurred';return false;}
